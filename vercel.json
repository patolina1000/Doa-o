{
  "version": 2,
  "name": "ajude-isabel-campaign",
  "builds": [
    {
      "src": "**/*",
      "use": "@vercel/static"
    }
  ],
  "redirects": [
    {
      "source": "/",
              "destination": "/Ang√©lica/",
      "permanent": false
    }
  ],
  "rewrites": [
    {
      "source": "/webhook/angelica",
      "destination": "/webhook/angelica/index.html"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Access-Control-Allow-Origin",
          "value": "*"
        },
        {
          "key": "Access-Control-Allow-Methods",
          "value": "GET, POST, PUT, DELETE, OPTIONS"
        },
        {
          "key": "Access-Control-Allow-Headers",
          "value": "Content-Type, Authorization"
        }
      ]
    }
  ]
}
